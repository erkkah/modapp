{
  "name": "modapp",
  "version": "2.1.4",
  "description": "A thin framework for building completely modular web applications.",
  "main": "./index.js",
  "scripts": {
    "build": "npm run clean && npm run eslint && npm run test && npm run docs && npm run babel",
    "babel": "babel src --out-dir ./",
    "test": "jest src --coverage",
    "clean": "rimraf class && rimraf coverage && rimraf docs/* && rimraf error && rimraf interface && rimraf *.js",
    "docs": "jsdoc2md -f ./src/class/App.js -f ./src/class/AppExt.js -f ./src/class/EventBus.js -f ./src/interface/AppModule.js -f ./src/interface/Component.js -f ./src/interface/Model.js -f ./src/interface/Collection.js -f ./src/interface/Readyable.js > ./docs/docs.md",
    "eslint": "eslint src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jirenius/modapp"
  },
  "author": "Samuel Jir√©nius <samuel@jirenius.com>",
  "license": "MIT",
  "homepage": "https://github.com/jirenius/modapp",
  "devDependencies": {
    "babel-cli": "6",
    "babel-preset-es2015": "6",
    "eslint": "^4.19.1",
    "jest": "^19.0.2",
    "jsdoc-to-markdown": "^4.0.1",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "modapp-utils": "^1.1.0"
  }
}
